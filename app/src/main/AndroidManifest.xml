<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.samsung.p3_pooa">

    <application
        android:name=".Persistence.BDConfig"
        android:allowBackup="true"
        android:icon="@mipmap/p3cidade"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cidade"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.RecyclerViewActivity">
            <activity android:name=".Activity.CidadeDestaque" />
        </activity>
        <activity android:name=".Activity.PontoTuristicoDestaque" />
        <activity android:name=".Activity.Usuario"></activity>
    </application>

</manifest>